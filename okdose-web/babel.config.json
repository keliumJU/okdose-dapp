{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-react", {"runtime": "automatic"}]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
        "alias": {
          "@nearInterface": "../okdose/near/nearInterface",
          "@leishmaniasis": "../okdose/transmission-types/transmitted-by-vectors/leishmaniasis",
          "@nearWallet": "../okdose/near/nearWallet"
        }
      }
    ]
  ]
}
